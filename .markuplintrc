{
  "extends": [
    "markuplint:recommended"
  ],
  "nodeRules": [
    {
      "selector": "html",
      "rules": {
        "invalid-attr": {
          "options": {
            "allowAttrs": ["prefix"]
          }
        }
      }
    },
    {
      "selector": "audio",
      "rules": {
        "required-element": false
      }
    },
    {
      "selector": "#MathJax-script",
      "rules": {
        "required-attr": ["async"]
      }
    }
  ]
}
